-# coding: utf-8
- if @previous_document || @next_document
  #previousNextDocument
    .page_links
      != link_to_previous_document @previous_document if @previous_document
      = '|'
      = item_page_entry_info
      = '|'
      != link_to_next_document @next_document
    %div.pull-right.search-widgets
      = link_back_to_catalog :class=>'btn'
      = '|'
      = link_to "#{t('blacklight.search.start_over')}", start_over_path(current_search_session.try(:query_params) || {}), :class=>"btn"